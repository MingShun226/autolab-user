{
  "functions": {
    "*.html": {
      "runtime": "@vercel/static"
    }
  },
  "rewrites": [
    { "source": "/", "destination": "/catalog.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}